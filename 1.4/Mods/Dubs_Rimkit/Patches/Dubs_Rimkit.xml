<?xml version="1.0" encoding="utf-8" ?>
<Patch>

    <!-- Moving Individual items to different techs -->

    <Operation Class="PatchOperationAdd">
        <xpath>
            Defs/ThingDef[
                   defName = "MedicalKit"
            ]/recipeMaker
        </xpath>
        <value>
            <researchPrerequisites Inherit="False">
                <li>DuneRef_Medicine</li>
            </researchPrerequisites>
        </value>
    </Operation>

    <Operation Class="PatchOperationAdd">
        <xpath>
            Defs/ThingDef[
                   defName = "UltraMedKit"
            ]/recipeMaker
        </xpath>
        <value>
            <researchPrerequisites Inherit="False">
                <li>DuneRef_Bioregeneration</li>
            </researchPrerequisites>
        </value>
    </Operation>

    <Operation Class="PatchOperationAdd">
        <xpath>
            Defs/ThingDef[
                   defName = "DubsRepairKit"
            ]/recipeMaker
        </xpath>
        <value>
            <researchPrerequisites Inherit="False">
                <li>DuneRef_ReplaceableParts</li>
            </researchPrerequisites>
        </value>
    </Operation>

</Patch>